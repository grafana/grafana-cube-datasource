{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Cube",
  "id": "grafana-cube-datasource",
  "metrics": true,
  "backend": true,
  "alerting": true,
  "multiValueFilterOperators": true,
  "executable": "gpx_cube",
  "info": {
    "description": "Connect Grafana to Cube for semantic layer analytics. Query measures and dimensions, apply filters, and visualize your data.",
    "author": {
      "name": "Grafana"
    },
    "keywords": ["datasource", "cube", "semantic layer", "analytics", "metrics"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "GitHub",
        "url": "https://github.com/grafana/grafana-cube-datasource"
      },
      {
        "name": "Cube Documentation",
        "url": "https://cube.dev/docs"
      }
    ],
    "screenshots": [
      {
        "name": "Query Editor",
        "path": "img/screenshot-query-editor.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=12.2.5 <12.3 || >=12.3.0",
    "plugins": []
  }
}
